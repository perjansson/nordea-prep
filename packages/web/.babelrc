{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": "> 3%"
                },
                "modules": false,
                "useBuiltIns": true,
                "debug": false
            }
        ],
        "react",
        "stage-1",
        "flow"
    ],
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs",
                "syntax-dynamic-import",
                [
                    "babel-plugin-styled-components",
                    {
                        "displayName": true
                    }
                ]
            ]
        }
    }
}
